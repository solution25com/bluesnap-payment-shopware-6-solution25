{
  "name": "solution25/bluesnap",
  "description": "BlueSnap payment method for Shopware 6",
  "type": "shopware-platform-plugin",
  "version": "1.0.3",
  "license": "proprietary",
  "authors": [
    {
        "name": "solution 25",
        "role": "Manufacturer"
    }
],
  "require": {
      "shopware/core": "^6.6 || ^6.7"
  },
  "extra": {
      "shopware-plugin-class": "BlueSnap\\BlueSnap",
      "label": {
          "de-DE": "BlueSnap payment method for Shopware 6",
          "en-GB": "BlueSnap payment method for Shopware 6"
      }
  },
  "autoload": {
      "psr-4": {
          "BlueSnap\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "BlueSnap\\Tests\\": "tests/"
      }
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
